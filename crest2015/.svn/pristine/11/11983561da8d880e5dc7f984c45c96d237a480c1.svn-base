<script type="text/javascript">
    $(function() {

                /** rmd 2015-03-07
                *********** start of  country dropdwon ********************************************/
                //option 3 data is loaded from db via controller
                //https://twitter.github.io/typeahead.js/examples/
               var gen_country_list = new Bloodhound({
                        datumTokenizer: function (datum) {
                            return Bloodhound.tokenizers.whitespace(datum.name);
                        },
                        queryTokenizer: Bloodhound.tokenizers.whitespace,
                        limit: 10,
                        prefetch: {
                            // url points to a json file that contains an array of country names, see
                            // https://github.com/twitter/typeahead.js/blob/gh-pages/data/countries.json
                            url: '../../admin/gen_country/dataJson',
                            // the json file contains an array of strings, but the Bloodhound
                            // suggestion engine expects JavaScript objects so this converts all of
                            // those strings
                             filter: function (gen_country_list) {
                                    console.log(gen_country_list); //debugging
                                    // Map the remote source JSON array to a JavaScript object array
                                    return $.map(gen_country_list, function (gen_country) {
                                        //console.log(country); //debugging
                                        return {
                                            code:gen_country.code,
                                            name:gen_country.name,
                                            id: gen_country.id
                                        };
                                    });
                            }
                        }
                });
                 
                // kicks off the loading/processing of `local` and `prefetch`
                gen_country_list.initialize();
                 
                //export_country_name_typeahead   -- this is the id of the div element that handles typeahead for export country name
                $('#export_country_name_typeahead .typeahead').typeahead(null, {
                  name: 'gen_country_list',
                  displayKey: 'name',
                  source: gen_country_list.ttAdapter()
                }).bind("typeahead:selected", function(obj, gen_country, name) {
                        console.log(gen_country); //debugging on browsers console
                        //assign value to our hidden field -  <input type="hidden" id="export_country_id" name="export_country_id">
                        $("#export_country_id").val(gen_country.id);
                        //automatically assign value to export_county_code
                        $("#export_country_code").val(gen_country.code);
                    });

